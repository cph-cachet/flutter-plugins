<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="dk.cachet.flutter_foreground_service">

    <uses-permission android:name="android.permission.WAKE_LOCK"/>
    <uses-permission android:name="android.permission.FOREGROUND_SERVICE"/>

    <application>
        <service android:name=".FlutterForegroundServicePlugin" android:exported="false"
                 android:foregroundServiceType="connectedDevice|dataSync|location|mediaPlayback|mediaProjection|phoneCall"/>
    </application>

</manifest>
